{
  "name": "krasterisk_speech_server",
  "version": "0.1.6",
  "description": "Speech server for aiPBX",
  "author": "Krasterisk LTD",
  "private": true,
  "license": "Apache",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "cross-env NODE_ENV=production nest start --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=debug nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lintfix": "eslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=production jest --config test/jest-e2e.json --detectOpenHandles"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/platform-socket.io": "^11.0.10",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/serve-static": "^5.0.2",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/websockets": "^11.0.10",
    "@types/multer": "^1.4.12",
    "alawmulaw": "^6.0.0",
    "axios": "^1.7.9",
    "base64-arraybuffer": "^1.0.2",
    "bcryptjs": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "datagram-stream": "^1.1.1",
    "express": "^5.0.1",
    "fs": "^0.0.1-security",
    "mysql2": "^3.12.0",
    "node-wav": "^0.0.2",
    "npm-check-updates": "^17.1.14",
    "openai": "^4.85.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.6",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "vosk-koffi": "^1.1.0",
    "ws": "^8.18.0",
    "x-law": "^0.3.1",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^6.2.4",
    "@eslint/js": "^9.20.0",
    "@nestjs/cli": "^11.0.4",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@types/ari-client": "^2.2.13",
    "@types/asterisk-manager": "^0.0.1",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "29.5.14",
    "@types/node": "^22.13.10",
    "@types/node-wav": "^0.0.3",
    "@types/sequelize": "^4.28.20",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.12.2",
    "@types/vosk": "^0.3.1",
    "@types/ws": "^8.5.14",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "jest": "29.7.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "yargs": "^17.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
